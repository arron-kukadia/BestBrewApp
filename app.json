{
  "expo": {
    "name": "BestBrew",
    "slug": "bestbrew",
    "scheme": "bestbrew",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#2DD4BF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bestbrew.app"
    },
    "android": {
      "package": "com.bestbrew.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2DD4BF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "blockedPermissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": ["expo-web-browser", "@react-native-community/datetimepicker"],
    "extra": {
      "eas": {
        "projectId": "6ebf34cb-b60e-48e3-a26c-78208fdf0ffc"
      }
    },
    "owner": "bestbrewapps-organization"
  }
}
